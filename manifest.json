{
  "manifest_version": 2,
  "name": "Compare products on Bol.com",
  "short_name": "Compare Bol",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.0.0",
  "permissions": ["https://www.bol.com/", "storage"],
  "content_scripts": [
    {
      "matches": ["http://*.bol.com/*", "https://*.bol.com/*"],
      "js": ["content/content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Add to Spotify",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "options_page": "options.html"
}
